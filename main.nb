Cell[BoxData[
 RowBox[{"pportrait", " ", "=", " ", 
  RowBox[{"Manipulate", "[", "\n", "\t", 
   RowBox[{"(*", 
    RowBox[{"Functionalities", " ", "and", " ", "calculations"}], "*)"}], 
   "\[IndentingNewLine]", "\t", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"f1", "[", 
       RowBox[{"x_", ",", "y_"}], "]"}], " ", "=", "f"}], ";", 
     "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{"f2", "[", 
       RowBox[{"x_", ",", "y_"}], "]"}], " ", "=", " ", "g"}], ";", 
     "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{"F", "[", 
       RowBox[{"x_", ",", "y_"}], "]"}], " ", "=", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"f1", "[", 
         RowBox[{"x", ",", "y"}], "]"}], ",", 
        RowBox[{"f2", "[", 
         RowBox[{"x", ",", "y"}], "]"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{"J", "[", 
       RowBox[{"x_", ",", "y_"}], "]"}], " ", "=", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"f1", "[", 
             RowBox[{"x", ",", "y"}], "]"}], ",", "x"}], "]"}], ",", 
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"f1", "[", 
             RowBox[{"x", ",", "y"}], "]"}], ",", "y"}], "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"f2", "[", 
             RowBox[{"x", ",", "y"}], "]"}], ",", "x"}], "]"}], ",", 
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"f2", "[", 
             RowBox[{"x", ",", "y"}], "]"}], ",", "y"}], "]"}]}], "}"}]}], 
       "}"}]}], ";", "\[IndentingNewLine]", "\t", 
     RowBox[{"fpList", " ", "=", " ", 
      RowBox[{"Solve", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"F", "[", 
             RowBox[{"x", ",", "y"}], "]"}], "[", 
            RowBox[{"[", "1", "]"}], "]"}], " ", "==", " ", "0"}], ",", " ", 
          RowBox[{
           RowBox[{
            RowBox[{"F", "[", 
             RowBox[{"x", ",", "y"}], "]"}], "[", 
            RowBox[{"[", "2", "]"}], "]"}], " ", "==", " ", "0"}]}], "}"}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", "y"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\t", 
     RowBox[{"systems", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", "\t", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
       RowBox[{"i", " ", "<=", " ", 
        RowBox[{"Length", "[", "fpList", "]"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\n", "\t\t", 
       RowBox[{
        RowBox[{"fp", " ", "=", " ", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"fpList", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}], "[", 
            RowBox[{"[", "2", "]"}], "]"}], ",", 
           RowBox[{
            RowBox[{
             RowBox[{"fpList", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "2", "]"}], "]"}], "[", 
            RowBox[{"[", "2", "]"}], "]"}]}], "}"}]}], ";", "\n", "\t\t", 
        RowBox[{"j", " ", "=", " ", 
         RowBox[{"MatrixForm", "[", 
          RowBox[{"J", "[", 
           RowBox[{
            RowBox[{"fp", "[", 
             RowBox[{"[", "1", "]"}], "]"}], ",", 
            RowBox[{"fp", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "]"}]}], ";", "\n", 
        "\t\t", 
        RowBox[{"(*", 
